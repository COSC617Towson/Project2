      .card.h-100
        h4.card-header Google Map
        .card-body
          p.card-text
          #map
         
        .card-footer
          a.btn.btn-primary(href='#') Learn More
    .col-lg-4.mb-4
      .card.h-100
        h4.card-header Info
        .card-body
          p.card-text
            | (description)
        .card-footer
          a.btn.btn-primary(href='#') Learn More
    .col-lg-4.mb-4
      .card.h-100
        h4.card-header Comment
        .card-body
          // comment model
          .row
            .col-md-12
              strong
                a(href='/users/5adc9268dc320d00149f14ce') sh
